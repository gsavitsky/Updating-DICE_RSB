			<aux name="CPC">
				<doc> Per capita consumption (thousand 2010$ per year)</doc>
				<eqn>1000*C/pop</eqn>
				<units>thousand 2010$/year</units>
			</aux>
			<aux name="PERIODU">
				<doc>Current period utility per person</doc>
				<eqn>(CPC^(1-elasmu))/(1-elasmu)</eqn>
			</aux>
			<aux name="PERIODUDISC">
				<doc>Discounted period utility per person</doc>
				<eqn>PERIODU*rr</eqn>
			</aux>
			<aux name="elasmu">
				<doc>Elasticity of marginal utility of consumption</doc>
				<eqn>1.45</eqn>
			</aux>
			<aux name="rr">
				<doc>Average utility social discount rate</doc>
				<eqn>1/((1+prstp)^(TIME-2015))</eqn>
			</aux>
			<stock name="UTILITY">
				<eqn>0</eqn>
				<inflow>Net_change_in_utility</inflow>
			</stock>
			<flow name="Net change in\nutility">
				<eqn>PERIODUDISC*pop</eqn>
			</flow>
			<aux name="prstp">
				<doc>Rate of social time preference (per year)</doc>
				<eqn>0.015</eqn>
				<units>1/Year</units>
			</aux>
			<aux name="pback">
				<doc>Backstop price (2010$ per tCO2)</doc>
				<eqn>pback0*(1-gback)^(TIME-2015)</eqn>
				<init_eqn>550</init_eqn>
				<units>2010$/tCO2</units>
			</aux>
			<aux name="cost1">
				<doc>Dollars spent on mitigation per dollar of gross output when MIU(t)=1</doc>
				<eqn>pback*sigma/expcost2/1000</eqn>
			</aux>
			<aux name="expcost2">
				<doc>Exponent of control cost function</doc>
				<eqn>2.6</eqn>
			</aux>
			<aux name="gback">
				<doc>Rate of decline backstop cost per period</doc>
				<eqn>0.004950737</eqn>
			</aux>
			<aux name="pback0">
				<eqn>550</eqn>
			</aux>
			<aux name="isthistimeperiod">
				<dimensions>
					<dim name="TimePeriod"/>
				</dimensions>
				<eqn>( IF INT(TIME-STARTTIME) = TimePeriod-1 THEN 1 ELSE 0 )</eqn>
			</aux>
			<aux name="optlrsav">
				<doc>Optimal long-run savings rate used for transversality (fraction)</doc>
				<eqn>(dk+0.004)/(dk+0.004*elasmu+prstp)*gama</eqn>
				<units>fraction</units>
			</aux>
			<aux name="endperiods">
				<doc>Number of periods at end to use for transversality</doc>
				<eqn>5</eqn>
				<units>Years</units>
			</aux>
			<aux name="s">
				<eqn>IF TIME &lt;= STOPTIME-endperiods THEN SUM(optimal_s[*]*isthistimeperiod[*]) ELSE optlrsav</eqn>
				<units>fraction</units>
			</aux>
			<aux name="miu">
				<doc>SUM(optimal_miu[*]*isthistimeperiod[*])</doc>
				<eqn>IF TIME = STARTTIME THEN miu0 ELSE SUM(optimal_miu[*]*isthistimeperiod[*])</eqn>
			</aux>
