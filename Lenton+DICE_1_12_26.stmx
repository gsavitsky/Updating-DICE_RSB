	<model name="Lenton_Carbon">
		<variables>
			<stock name="Atmospheric Carbon" access="output" isee:autocreated="true">
				<eqn>Ca0</eqn>
				<inflow>Plant_Respiration</inflow>
				<inflow>Soil_Respiration</inflow>
				<inflow>&quot;Air-Sea_Exchange_(LL)&quot;</inflow>
				<inflow>&quot;Air-Sea_Exchange_(HL)&quot;</inflow>
				<inflow>GHG_emissions</inflow>
				<inflow>Permafrost_melt</inflow>
				<outflow>Photosynthesis</outflow>
				<non_negative/>
				<units>pgC</units>
			</stock>
			<stock name="Vegetation Carbon">
				<eqn>Cv0</eqn>
				<inflow>Photosynthesis</inflow>
				<outflow>Plant_Respiration</outflow>
				<outflow>Turnover</outflow>
				<non_negative/>
				<units>pgC</units>
			</stock>
			<stock name="Soil Carbon">
				<eqn>Cs0</eqn>
				<inflow>Turnover</inflow>
				<outflow>Soil_Respiration</outflow>
				<non_negative/>
				<units>pgC</units>
			</stock>
			<flow name="Photosynthesis">
				<eqn> (&quot;photosynthesis_rate_(kp)&quot;*&quot;Cv(s)&quot;*&quot;photosynthesis_normalizing_constant_(kMM)&quot;)*

((pCO2_ppmv-&quot;compensation_point_(kc)&quot;)/(&quot;Half_saturation_point_(KM)&quot;+(pCO2_ppmv-&quot;compensation_point_(kc)&quot;)))*

((15+Anomaly)^2)*(25-Anomaly)/5625</eqn>
				<non_negative/>
				<units>pgC/Year</units>
			</flow>
			<flow name="Plant Respiration">
				<eqn>&quot;plant_respiration_rate_(kr)&quot; * Vegetation_Carbon * &quot;plant_respiration_normalizing_constant_(kA)&quot; * EXP(-&quot;Plant_respiration_activation_energy_(Ea)&quot; / (R * &quot;surface_temperature_(Ts)_1&quot;))</eqn>
				<non_negative/>
				<units>pgC/Year</units>
			</flow>
			<flow name="Turnover">
				<eqn>&quot;turnover_rate_(kt)&quot;*Vegetation_Carbon</eqn>
				<non_negative/>
				<units>pgC/Year</units>
			</flow>
			<flow name="Soil Respiration">
				<eqn>&quot;soil_respiration_rate_(ksr)&quot;*Soil_Carbon*&quot;soil_respiration_normalizing_constant_(kB)&quot;* 2.7^(-1*(308.56/(&quot;surface_temperature_(Ts)_1&quot; -227.13)))</eqn>
				<non_negative/>
				<units>pgC/Year</units>
			</flow>
			<stock name="Low Latitude Ocean Carbon">
				<eqn>Col0</eqn>
				<inflow>&quot;intermediate-warm_exchange&quot;</inflow>
				<outflow>&quot;Air-Sea_Exchange_(LL)&quot;</outflow>
				<outflow>&quot;Warm-intermediate_Ocean_exchange&quot;</outflow>
				<non_negative/>
				<units>pgC</units>
			</stock>
			<stock name="High Latitude Surface Ocean Carbon">
				<eqn>Coh0</eqn>
				<outflow>&quot;Air-Sea_Exchange_(HL)&quot;</outflow>
				<outflow>&quot;cold-intermediate_ocean_exchange&quot;</outflow>
				<non_negative/>
				<units>pgC</units>
			</stock>
			<stock name="Intermediate Ocean Carbon">
				<eqn>Coi0</eqn>
				<inflow>&quot;Warm-intermediate_Ocean_exchange&quot;</inflow>
				<inflow>&quot;cold-intermediate_ocean_exchange&quot;</inflow>
				<inflow>&quot;deep-intermediate_exchange&quot;</inflow>
				<outflow>&quot;Intermediate-deep_Ocean_exchange&quot;</outflow>
				<outflow>&quot;intermediate-warm_exchange&quot;</outflow>
				<non_negative/>
				<units>pgC</units>
			</stock>
			<stock name="Deep Ocean Carbon">
				<eqn>Cod0</eqn>
				<inflow>&quot;Intermediate-deep_Ocean_exchange&quot;</inflow>
				<outflow>&quot;deep-intermediate_exchange&quot;</outflow>
				<non_negative/>
				<units>pgC</units>
			</stock>
			<flow name="Air-Sea Exchange (LL)">
				<eqn>-Faw</eqn>
				<units>pgC/Year</units>
			</flow>
			<flow name="Air-Sea Exchange (HL)">
				<eqn>-Fac</eqn>
				<units>pgC/Year</units>
			</flow>
			<aux name="photosynthesis rate (kp)">
				<eqn>0.184</eqn>
				<units>1/year</units>
			</aux>
			<aux name="plant respiration rate (kr)">
				<eqn>0.092</eqn>
				<units>1/year</units>
			</aux>
			<aux name="turnover rate (kt)">
				<eqn>0.092</eqn>
				<units>1/year</units>
			</aux>
			<aux name="soil respiration rate (ksr)">
				<eqn>0.0337</eqn>
				<units>1/year</units>
			</aux>
			<aux name="Half saturation point (KM)">
				<eqn>120</eqn>
				<units>ppmv</units>
			</aux>
			<aux name="Plant respiration activation energy (Ea)">
				<eqn>54830</eqn>
				<units>J/mol</units>
			</aux>
			<aux name="photosynthesis normalizing constant (kMM)">
				<eqn>1.478</eqn>
				<units>Dimensionless</units>
			</aux>
			<aux name="plant respiration\nnormalizing constant (kA)">
				<eqn>8.7039e9</eqn>
				<units>Dimensionless</units>
			</aux>
			<aux name="thermohaline overturning (kT)">
				<eqn>6.5e7</eqn>
				<units>m^3/s</units>
			</aux>
			<aux name="high latitude overturning (kU)">
				<eqn>4.87e7</eqn>
				<units>m^3/s</units>
			</aux>
			<aux name="warm-intermediate exchange (kwi)">
				<eqn>1.25e7</eqn>
				<units>m^3/s</units>
			</aux>
			<aux name="intermediate-deep exchange (kid)">
				<eqn>2e8</eqn>
				<units>m^3/s</units>
			</aux>
			<aux name="piston velocity (ks)">
				<eqn>1752</eqn>
				<units>m/year</units>
			</aux>
			<aux name="compensation point (kc)">
				<eqn>29</eqn>
				<units>ppmv</units>
			</aux>
			<aux name="soil respiration normalizing constant (kB)">
				<eqn>157.072</eqn>
				<units>Dimensionless</units>
			</aux>
			<aux name="partial pressure of CO2 (pCO2)" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>ppm</units>
			</aux>
			<aux name="Anomaly" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>K</units>
			</aux>
			<aux name="Cs0">
				<eqn>1500</eqn>
			</aux>
			<aux name="Ca0">
				<eqn>850</eqn>
				<units>pgC</units>
			</aux>
			<aux name="Cv0">
				<eqn>550</eqn>
				<units>pgC</units>
			</aux>
			<aux name="kD">
				<eqn>0.27</eqn>
				<units>Dimensionless</units>
			</aux>
			<aux name="Cv(s)">
				<eqn>Cv0-(cumulative_deforestation*kD)</eqn>
			</aux>
			<stock name="cumulative deforestation">
				<eqn>0</eqn>
				<inflow>&quot;Deforestation_(D)&quot;</inflow>
				<non_negative/>
				<units>PgC</units>
			</stock>
			<flow name="Deforestation (D)">
				<eqn>eland/2.12</eqn>
				<non_negative/>
				<units>PgC/Year</units>
			</flow>
			<group name="Land Use Change" run="false">
				<entity name='"Cv(s)"'/>
				<entity name='"Deforestation_(D)"'/>
				<entity name="cumulative_deforestation"/>
				<entity name="deland"/>
				<entity name="eland"/>
				<entity name="eland0"/>
				<entity name="kD"/>
			</group>
			<aux name="R">
				<eqn>8.314</eqn>
				<units>J/mol*K</units>
			</aux>
			<group name="Initialization" run="false">
				<entity name="Ca0"/>
				<entity name="Cod0"/>
				<entity name="Coh0"/>
				<entity name="Coi0"/>
				<entity name="Col0"/>
				<entity name="Cs0"/>
				<entity name="Cv0"/>
			</group>
			<aux name="Th">
				<eqn>275.15</eqn>
				<units>K</units>
			</aux>
			<aux name="Tl">
				<eqn>295.15</eqn>
				<units>K</units>
			</aux>
			<aux name="Faw">
				<eqn>&quot;piston_velocity_(ks)&quot;*aO*Al*bw*(&quot;partial_pressure_of_CO2_(pCO2)&quot;-&quot;pCO2(w)&quot;)*12e-15*rho</eqn>
			</aux>
			<aux name="Al">
				<eqn>0.85</eqn>
			</aux>
			<aux name="Ah">
				<eqn>0.15</eqn>
			</aux>
			<aux name="pCO2(w)">
				<eqn>&quot;pCO2(w)0&quot; * (Low_Latitude_Ocean_Carbon / Col0)^&quot;beta_(revelle_factor)&quot;</eqn>
			</aux>
			<aux name="pCO2(c)">
				<eqn>&quot;pCO2(c)0&quot; * (High_Latitude_Surface_Ocean_Carbon / Coh0)^&quot;beta_(revelle_factor)&quot;</eqn>
			</aux>
			<aux name="bw">
				<eqn>EXP(-60.2409 + 93.4517*(100/Tl) + 23.3585*LN(Tl/100) 
         + 34.7*(0.023517 - 0.023656*(Tl/100) + 0.0047036*(Tl/100)^2))</eqn>
				<units>mol/kg atm</units>
			</aux>
			<aux name="aO">
				<eqn>3.6e14</eqn>
				<units>m^2</units>
			</aux>
			<aux name="Fac">
				<eqn>&quot;piston_velocity_(ks)&quot;*aO*Ah*bc*(&quot;partial_pressure_of_CO2_(pCO2)&quot;-&quot;pCO2(c)&quot;)*12e-15*rho</eqn>
			</aux>
			<aux name="bc">
				<eqn>EXP(-60.2409 + 93.4517*(100/Th) + 23.3585*LN(Th/100) 
         + 34.7*(0.023517 - 0.023656*(Th/100) + 0.0047036*(Th/100)^2))</eqn>
				<units>mol/kg atm</units>
			</aux>
			<flow name="Warm-intermediate Ocean exchange">
				<eqn>&quot;warm-intermediate_exchange_(kwi)&quot; * (Low_Latitude_Ocean_Carbon / Vl) * 3.15576e7 *12e-15</eqn>
				<non_negative/>
				<units>pgC/Year</units>
			</flow>
			<flow name="Intermediate-deep Ocean exchange">
				<eqn>&quot;intermediate-deep_exchange_(kid)&quot; * (Intermediate_Ocean_Carbon / Vi) * 3.15576e7 * 12e-15</eqn>
				<non_negative/>
				<units>pgC/Year</units>
			</flow>
			<aux name="beta (revelle factor)">
				<eqn>11</eqn>
			</aux>
			<aux name="pCO2(w)0">
				<eqn>286e-6</eqn>
			</aux>
			<aux name="pCO2(c)0">
				<eqn>270e-6</eqn>
			</aux>
			<aux name="Col0">
				<eqn>730</eqn>
				<units>pgC</units>
			</aux>
			<aux name="Coh0">
				<eqn>140</eqn>
				<units>pgC</units>
			</aux>
			<aux name="rho">
				<eqn>1027</eqn>
				<units>kg/m^2</units>
			</aux>
			<aux name="Coi0">
				<eqn>10040</eqn>
			</aux>
			<aux name="Cod0">
				<eqn>23860</eqn>
				<units>pgC</units>
			</aux>
			<flow name="cold-intermediate ocean exchange">
				<eqn>&quot;high_latitude_overturning_(kU)&quot; * (High_Latitude_Surface_Ocean_Carbon / Vh) * 3.15576e7 *12e-15</eqn>
				<non_negative/>
				<units>pgC/Year</units>
			</flow>
			<aux name="Vl">
				<eqn>3.06e16</eqn>
				<units>m^3</units>
			</aux>
			<aux name="Vh">
				<eqn>5.4e15</eqn>
				<units>m^3</units>
			</aux>
			<aux name="Vi">
				<eqn>3.6e17</eqn>
				<units>m^3</units>
			</aux>
			<aux name="Vd">
				<eqn>9.7e17</eqn>
				<units>m^3</units>
			</aux>
			<flow name="deep-intermediate exchange">
				<eqn>&quot;intermediate-deep_exchange_(kid)&quot; * (Deep_Ocean_Carbon / Vd) * 3.15576e7 * 12e-15</eqn>
				<non_negative/>
				<units>pgC/Year</units>
			</flow>
			<flow name="intermediate-warm exchange">
				<eqn>(&quot;thermohaline_overturning_(kT)&quot; + &quot;warm-intermediate_exchange_(kwi)&quot;) * (Intermediate_Ocean_Carbon / Vi) * 3.15576e7 * 12e-15</eqn>
				<non_negative/>
				<units>pgC/Year</units>
			</flow>
			<aux name="pCO2 ppmv">
				<eqn>&quot;partial_pressure_of_CO2_(pCO2)&quot;*1e6</eqn>
			</aux>
