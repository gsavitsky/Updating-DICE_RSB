	<model name="Lenton_Climate">
		<variables>
			<aux name="solar constant">
				<eqn>1368</eqn>
				<units>W m^-2</units>
			</aux>
			<aux name="surface albedo (A)">
				<eqn>0.225</eqn>
				<units>unitless</units>
			</aux>
			<aux name="Specific Heat Capacity (c)">
				<eqn>4.69e23</eqn>
				<units>J K^-1</units>
			</aux>
			<aux name="incoming radiation">
				<eqn>(((1-&quot;surface_albedo_(A)&quot;)*solar_constant)/4) * (1+(3/4*&quot;vertical_opacity_of_the_greenhouse_atmosphere_(t)&quot;))</eqn>
				<units>W m^-2</units>
			</aux>
			<stock name="surface temperature (Ts)" access="output" isee:autocreated="true">
				<eqn>initial_Ts+1.14</eqn>
				<inflow>global_temperature_change</inflow>
				<non_negative/>
				<units>K</units>
			</stock>
			<aux name="Earth's Surface Area (aE)">
				<eqn>5.101E14</eqn>
				<units>m ^2</units>
			</aux>
			<aux name="Stefan-Boltzman Constant (sigma)">
				<eqn>5.67e-8</eqn>
				<units>W m^-2 K^-4</units>
			</aux>
			<aux name="methane opacity (t(CH4))">
				<eqn>0.0231</eqn>
				<units>unitless</units>
			</aux>
			<aux name="water vapor saturation\nconstant (P0)">
				<eqn>1.4e11</eqn>
				<units>Pa</units>
			</aux>
			<aux name="relative humidity (H)">
				<eqn>0.6177</eqn>
				<units>unitless</units>
			</aux>
			<aux name="exponential function (e)">
				<eqn>2.71828</eqn>
				<units>unitless</units>
			</aux>
			<aux name="molar gas constant (R)">
				<eqn>8.314</eqn>
				<units>J/mol*K</units>
			</aux>
			<aux name="water opacity (t(H2O))">
				<eqn>0.0126*(&quot;relative_humidity_(pH2O)&quot;^0.503)</eqn>
				<units>unitless</units>
			</aux>
			<aux name="vertical opacity of the\ngreenhouse atmosphere (t)">
				<eqn>&quot;methane_opacity_(t(CH4))&quot;+&quot;water_opacity_(t(H2O))&quot;+&quot;carbon_dioxide_opacity_(t(CO2))&quot;</eqn>
				<units>unitless</units>
			</aux>
			<aux name="relative humidity (pH2O)">
				<eqn>&quot;relative_humidity_(H)&quot;*(&quot;water_vapor_saturation_constant_(P0)&quot;*(&quot;exponential_function_(e)&quot;^(-1*(&quot;fall_and_plant_death_-_turnover_(L)&quot;/(&quot;molar_gas_constant_(R)&quot;*&quot;surface_temperature_(Ts)&quot;)))))</eqn>
				<units>Pa</units>
			</aux>
			<aux name="initial Ts">
				<eqn>288.15</eqn>
				<units>K</units>
			</aux>
			<aux name="fall and plant\ndeath - turnover (L)">
				<eqn>43655</eqn>
				<units>J/mol</units>
			</aux>
			<aux name="carbon dioxide opacity (t(CO2))">
				<eqn>1.73*&quot;partial_pressure_of_CO2_(pCO2)&quot;^0.263</eqn>
				<units>unitless</units>
			</aux>
			<aux name="Anomaly" access="output">
				<eqn>&quot;surface_temperature_(Ts)&quot;-initial_Ts</eqn>
				<units>K</units>
			</aux>
			<aux name="partial pressure of CO2 (pCO2)" access="output" isee:autocreated="true">
				<eqn>Atmospheric_Carbon_1/&quot;mole_volume_of_the_atmosphere_(ka)&quot;*&quot;pre-industrial_moles_of_molecules_in_the_atmosphere_(fgtm)&quot;</eqn>
				<units>ppm</units>
			</aux>
			<aux name="outgoing radiation">
				<eqn>&quot;Stefan-Boltzman_Constant_(sigma)&quot;*(&quot;surface_temperature_(Ts)&quot;^4)</eqn>
				<units>W m^-2</units>
			</aux>
			<aux name="energy balance">
				<eqn>incoming_radiation-outgoing_radiation</eqn>
				<units>W m^-2</units>
			</aux>
			<aux name="global energy balance">
				<eqn>energy_balance*&quot;Earth&apos;s_Surface_Area_(aE)&quot;</eqn>
				<units>w</units>
			</aux>
			<flow name="global temperature change">
				<eqn>&quot;Earth&apos;s_Surface_Area_(aE)&quot;*(1/&quot;Specific_Heat_Capacity_(c)&quot;)*(incoming_radiation-&quot;Stefan-Boltzman_Constant_(sigma)&quot;*&quot;surface_temperature_(Ts)&quot;^4)* 3.154e+7</eqn>
				<units>K/Year</units>
			</flow>
			<aux name="mole volume of the atmosphere (ka)">
				<eqn>1.773e20</eqn>
				<units>mol</units>
			</aux>
			<aux name="pre-industrial\nmoles of molecules in\nthe atmosphere (fgtm)">
				<eqn>8.3259e13</eqn>
				<units>mol/pgC</units>
			</aux>
			<stock name="Atmospheric Carbon 1" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<non_negative/>
				<units>pgC</units>
			</stock>
