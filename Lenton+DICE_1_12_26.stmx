	<model name="DICE_Economy">
		<variables>
			<stock name="K">
				<doc>Produced capital</doc>
				<eqn>223</eqn>
				<inflow>Production_of_New_Capital</inflow>
				<outflow>Retiring_Capital</outflow>
				<non_negative/>
				<units>trillion 2010$</units>
			</stock>
			<flow name="Retiring\nCapital">
				<doc>Retiring capital</doc>
				<eqn>dk*K</eqn>
				<non_negative/>
				<units>trillion 2010$/Year</units>
			</flow>
			<aux name="dk">
				<doc>Retirement/depreciation rate of produced capital</doc>
				<eqn>0.1</eqn>
				<units>(trillion 2010$/year)/trillion 2010$</units>
			</aux>
			<flow name="Production of\nNew Capital">
				<doc>New capital production</doc>
				<eqn>I</eqn>
				<non_negative/>
				<units>trillion 2010$/Year</units>
			</flow>
			<stock name="pop">
				<doc>World population in millions</doc>
				<eqn>7403</eqn>
				<inflow>Net_change_in_population</inflow>
				<non_negative/>
				<units>million persons</units>
			</stock>
			<aux name="gama">
				<doc>Capital elasticity in production function</doc>
				<eqn>0.3</eqn>
			</aux>
			<aux name="YGROSS">
				<doc>Gross world output</doc>
				<eqn>tfp*(pop/1000)^(1-gama)*K^gama</eqn>
				<init_eqn>105.5</init_eqn>
				<units>trillion 2010$/Year</units>
			</aux>
			<aux name="C">
				<doc>Consumption</doc>
				<eqn>YNETALL-I</eqn>
				<units>trillion 2010$/Year</units>
			</aux>
			<aux name="I">
				<eqn>s*YNETALL</eqn>
				<units>trillion 2010$/Year</units>
			</aux>
			<aux name="TCABATE">
				<doc>Total cost of abatement</doc>
				<eqn>YGROSS*cost1*miu^expcost2</eqn>
				<units>trillion 2010$/Year</units>
			</aux>
			<aux name="DAMFRACCLIM">
				<doc>Damages from climate equation</doc>
				<eqn>a2*Converter_1^a3</eqn>
				<units>fraction</units>
			</aux>
			<aux name="optimal s">
				<doc>fraction of output net of damages and mitigation saved and invested in the production of new capital

in DICE, this is one of the choice variables in the optimization</doc>
				<dimensions>
					<dim name="TimePeriod"/>
				</dimensions>
				<eqn>0.262</eqn>
				<units>fraction</units>
			</aux>
			<aux name="YNETDAM">
				<doc>World output net of damages</doc>
				<eqn>YGROSS-DAMAGESTOT</eqn>
				<units>trillion 2010$/Year</units>
			</aux>
			<aux name="YNETALL">
				<doc>World output net of damages and spending on abatement</doc>
				<eqn>YNETDAM-TCABATE</eqn>
				<units>trillion 2010$/Year</units>
			</aux>
			<aux name="DAMAGESTOT">
				<eqn>DAMFRACCLIM*YGROSS</eqn>
				<units>trillion 2010$/Year</units>
			</aux>
			<aux name="optimal miu" access="output" isee:autocreated="true">
				<doc>fraction of uncontrolled industrial and energy emissions mitigated

in DICE, this is one of the choice variables in the optimization</doc>
				<dimensions>
					<dim name="TimePeriod"/>
				</dimensions>
				<eqn>0.2</eqn>
				<units>fraction</units>
			</aux>
			<aux name="EIND" access="output">
				<doc>industrial and energy emissions after abatement</doc>
				<eqn>sigma*YGROSS*(1-miu)</eqn>
				<init_eqn>35.85</init_eqn>
				<units>GtCO2/Year</units>
			</aux>
			<stock name="tfp">
				<doc>Total Factor Productivity</doc>
				<eqn>YGROSS/((pop/1000)^(1-gama) * K^gama)</eqn>
				<inflow>Net_change_in_tfp</inflow>
				<non_negative/>
			</stock>
			<aux name="a2">
				<doc>Multiplier on power term in climate damage fraction equation</doc>
				<eqn>0.00227</eqn>
			</aux>
			<aux name="a3">
				<doc>Exponent on power term in climate damage fraction equation</doc>
				<eqn>2.00</eqn>
			</aux>
			<stock name="sigma">
				<doc>CO2-equivalent-emissions output ratio (tons CO2 per 1000 2010$ gross output)</doc>
				<eqn>(EIND/(YGROSS*(1-miu0)))*sigma_scale</eqn>
				<inflow>Net_change_in_sigma</inflow>
				<non_negative/>
				<units>tons CO2/1000 2010$</units>
			</stock>
			<aux name="popadj">
				<doc>Population exponent for 1 year time step</doc>
				<eqn>0.028364042</eqn>
			</aux>
			<aux name="popasym">
				<doc>Asymptotic population</doc>
				<eqn>11500</eqn>
				<units>million persons</units>
			</aux>
			<flow name="Net change in\npopulation">
				<doc>Net change in world population</doc>
				<eqn>pop*((popasym/pop)^popadj - 1)</eqn>
				<units>million persons/Year</units>
			</flow>
			<group name="Population" run="true">
				<entity name="Net_change_in_population"/>
				<entity name="pop"/>
				<entity name="popadj"/>
				<entity name="popasym"/>
			</group>
			<aux name="tfpadj">
				<doc>tfp exponent for 1 year time step</doc>
				<eqn>0.005082</eqn>
			</aux>
			<aux name="tfpasym">
				<doc>Asymptotic tfp</doc>
				<eqn>117.025634</eqn>
			</aux>
			<flow name="Net change in\ntfp">
				<eqn>tfp * ((tfpasym/tfp)^tfpadj - 1)</eqn>
			</flow>
