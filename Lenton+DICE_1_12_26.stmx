	<model name="Permafrost_dynamics">
		<variables>
			<aux name="Anomaly" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>K</units>
			</aux>
			<stock name="Cperm">
				<eqn>1035</eqn>
				<outflow>delta_Cperm</outflow>
				<outflow>abrupt_thaw</outflow>
				<non_negative/>
				<units>pgC</units>
			</stock>
			<flow name="delta Cperm" access="output">
				<eqn>-1*(frozen_fraction - DELAY(frozen_fraction, DT)) * DELAY(Cperm,  DT)</eqn>
				<units>pgC/Years</units>
			</flow>
			<stock name="thawed C">
				<eqn>0</eqn>
				<inflow>delta_Cperm</inflow>
				<inflow>abrupt_thaw</inflow>
				<outflow>Fthawedatmo</outflow>
				<non_negative/>
				<units>pgC</units>
			</stock>
			<flow name="Fthawedatmo" access="output">
				<eqn>IF permafrost_switch = 1 THEN RH_pf+(RH_CH4) ELSE 0</eqn>
				<non_negative/>
				<units>pgC/Year</units>
			</flow>
			<aux name="RH pf">
				<eqn>(1 - F_CH4) * labile_thawed * Q10^(T200 / 10)</eqn>
			</aux>
			<aux name="Q10">
				<eqn>2</eqn>
			</aux>
			<aux name="F CH4">
				<eqn>0.023</eqn>
			</aux>
			<aux name="labile thawed">
				<eqn>thawed_C - static_c_in_thawed</eqn>
			</aux>
			<aux name="T200">
				<eqn>DELAY1(Anomaly, 200)</eqn>
			</aux>
			<aux name="RH CH4">
				<eqn>F_CH4 * labile_thawed * Q10^(T200 / 10)</eqn>
			</aux>
			<aux name="T HL">
				<eqn>Anomaly*2</eqn>
			</aux>
			<aux name="frozen fraction">
				<eqn>NORMALCDF(LN(1+T_HL), 1e6, 1.67, 0.99)</eqn>
			</aux>
			<aux name="permafrost switch" access="output" isee:autocreated="true">
				<eqn>1</eqn>
				<format precision="1"/>
				<scale min="0" max="1"/>
			</aux>
			<aux name="fstatic">
				<eqn>0.74</eqn>
			</aux>
			<flow name="abrupt thaw" access="output">
				<eqn>IF Anomaly &gt; 2 THEN delta_Cperm*0.4 ELSE 0</eqn>
				<non_negative/>
				<units>pgC/Years</units>
			</flow>
			<aux name="static c in thawed">
				<eqn>fstatic*thawed_C</eqn>
			</aux>
		</variables>
		<views>
			<style color="black" background="white" font_style="normal" font_weight="normal" text_decoration="none" text_align="center" vertical_text_align="center" font_color="black" font_family="Arial" font_size="10pt" padding="2" border_color="black" border_width="thin" border_style="none">
